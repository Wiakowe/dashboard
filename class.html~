        <div id="dashboard">
            hola
	</div>
        <script src="class.js"></script>
        <script type="text/javascript">

var Person = Class.create({
  val: 1,
  init: function(cv) { 
    this.cv = cv; 
  },
  test: function() { 
    return [this.val, this.cv].join(", "); 
  },
  person: function() {
    console.log('person');
  }
});

var Man = Person.extend({
  val: 2,
  init: function(cv, cv2) { 
    this._super(cv); this.cv2 = cv2; 
  },
  test: function() { 
    return [this._super(), this.cv2].join(", "); 
  }
});

var Child = Man.extend({
  val: 3,
  init: function(cv, cv2) { 
    this._super(cv); this.cv2 = cv2; 
  },
  test: function() { 
    return [this._super(), this.cv2].join(", "); 
  }
});


var x = new Person(123);
var y = new Man(234, 567);
var z = new Child();

console.log(x);
console.log(y);
console.log(z);



        </script>

	</body>
</html>
